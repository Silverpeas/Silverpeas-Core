 <portlet>
        <description>Test portlet embarquee dans SP</description>
        <portlet-name>HelloSilverpeasPortlet</portlet-name>
        <display-name>HelloSilverpeasPortlet</display-name>
        <portlet-class>com.silverpeas.portlets.HelloSilverpeasPortlet</portlet-class>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>          
            <portlet-mode>VIEW</portlet-mode>
            <portlet-mode>EDIT</portlet-mode>
            <portlet-mode>HELP</portlet-mode>
        </supports>
        <portlet-info>
            <title>HelloSilverpeasPortlet title</title>
            <short-title>HelloSilverpeasPortlet shortTitle</short-title>
        </portlet-info>
 </portlet>
 
