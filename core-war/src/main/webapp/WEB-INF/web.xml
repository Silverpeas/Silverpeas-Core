  </context-param>
  <context-param>
    <!-- Init Parameters -->
    <param-name>request_response_factory.minSizeParam</param-name>
    <param-value>0</param-value>
  </context-param>
  <context-param>
    <param-name>request_response_factory.maxSizeParam</param-name>
    <param-value>200</param-value>
  </context-param>
  <context-param>
    <param-name>request_response_factory.partitionParam</param-name>
    <param-value>10</param-value>
  </context-param>
  <context-param>
    <param-name>vendorPortletXML.sun.validate</param-name>
    <param-value>false</param-value>
  </context-param>
  <context-param>
    <param-name>vendorPortletXML.sun.impl</param-name>
    <param-value>com.sun.portal.portletcontainer.common.descriptor.SunPortletXMLImpl</param-value>
  </context-param>
  <context-param>
    <param-name>vendorPortletXML.sun.name</param-name>
    <param-value>sun-portlet.xml</param-value>
  </context-param>

  <filter>
    <description>
      To ensure the encoding of the request parameters are in UTF-8. This filter MUST be the first
      one in order to work correctly.
    </description>
    <filter-name>CharacterEncodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <async-supported>true</async-supported>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
    <init-param>
      <param-name>forceEncoding</param-name>
      <param-value>true</param-value>
    </init-param>
  </filter>
  <filter>
    <description>
      To decorate the incoming HTTP request by a HttpRequest instance that provides more useful
      functionalities.
    </description>
    <filter-name>HttpServletRequestDecoration</filter-name>
    <filter-class>org.silverpeas.web.filter.HttpServletRequestDecoration</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <description>
      A filter that handle resource caches.
    </description>
    <filter-name>ResourceCacheFilter</filter-name>
    <filter-class>org.silverpeas.web.filter.ResourceCacheFilter</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <description>
      To handle the logging of all exception thrown from Silverpeas application.
    </description>
    <filter-name>SilverpeasExceptionLoggingFilter</filter-name>
    <filter-class>org.silverpeas.web.filter.SilverpeasExceptionLoggingFilter</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <!-- MessageFilter Filter -->
    <description>
      To manage for each incoming request, the life-cycle of a l10n message container in which
      are set the messages for the end-user.
    </description>
    <filter-name>MessageFilter</filter-name>
    <filter-class>org.silverpeas.core.web.filter.MessageFilter</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <!-- ScimFilter Filter -->
    <description>
      To manage for each incoming request, the life-cycle of a SCIM client.
    </description>
    <filter-name>ScimServerFilter</filter-name>
    <filter-class>org.silverpeas.core.webapi.admin.scim.ScimServerFilter</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <!-- WopiFilter Filter -->
    <description>
      To manage for each incoming request, the life-cycle of a WOPI host.
    </description>
    <filter-name>WopiServerFilter</filter-name>
    <filter-class>org.silverpeas.core.webapi.wopi.WopiServerFilter</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <description>
      To validate each incoming HTTP request to a protected resource by using a synchronizer token
      mechanism.
    </description>
    <filter-name>SessionSynchronizerTokenValidator</filter-name>
    <filter-class>org.silverpeas.web.token.SessionSynchronizerTokenValidator</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <description>
      To force IE to unset its compatibility mode and then to use the latest available features
      of the web navigator.
    </description>
    <filter-name>InternetExplorerAntiCompatibilityFilter</filter-name>
    <filter-class>org.silverpeas.web.filter.InternetExplorerAntiCompatibilityFilter</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <!-- PortletAppEngine Filter -->
    <filter-name>PortletAppEngineFilter</filter-name>
    <filter-class>com.sun.portal.portletcontainer.appengine.PortletAppEngineFilter</filter-class>
  </filter>
  <filter>
    <!-- WebCORSFilter Filter -->
    <description>
      To handle cross-origin resource sharing.
    </description>
    <filter-name>WebCORSFilter</filter-name>
    <filter-class>org.silverpeas.core.web.filter.WebCORSFilter</filter-class>
    <async-supported>true</async-supported>
  </filter>
  <filter>
    <!-- MassiveWebSecurityFilter Filter -->
    <filter-name>MassiveWebSecurityFilter</filter-name>
    <filter-class>org.silverpeas.core.web.filter.MassiveWebSecurityFilter</filter-class>
    <async-supported>true</async-supported>
  </filter>

  <filter-mapping>
    <filter-name>CharacterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>HttpServletRequestDecoration</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>ResourceCacheFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>SilverpeasExceptionLoggingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>MessageFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>ScimServerFilter</filter-name>
    <url-pattern>/services/domains/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>WopiServerFilter</filter-name>
    <url-pattern>/services/wopi/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>SessionSynchronizerTokenValidator</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>InternetExplorerAntiCompatibilityFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>PortletAppEngineFilter</filter-name>
    <servlet-name>PortletAppEngineServlet</servlet-name>
    <dispatcher>INCLUDE</dispatcher>
  </filter-mapping>
  <filter-mapping>
    <filter-name>WebCORSFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <!-- MassiveWebSecurityFilter Filter mapping -->
    <filter-name>MassiveWebSecurityFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

