    <exception-type>org.silverpeas.spnego.SpnegoUnauthenticatedException</exception-type>
    <location>/Login?ErrorCode=Error_SsoNotAllowed</location>
  </error-page>
  <error-page>
    <error-code>401</error-code>
    <location>/Error401.jsp</location>
  </error-page>
  <error-page>
    <error-code>403</error-code>
    <location>/Error403.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/Error404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/Error500.jsp</location>
  </error-page>
  <error-page>
    <exception-type>org.silverpeas.core.admin.space.quota.process.check.exception.DataStorageQuotaException</exception-type>
    <location>/admin/jsp/errorpageDataStorageQuota.jsp</location>
  </error-page>
  <error-page>
    <exception-type>org.silverpeas.core.admin.component.exception.ComponentFileFilterException</exception-type>
    <location>/admin/jsp/errorpageForbiddenFile.jsp</location>
  </error-page>

  <security-constraint>
    <display-name>Authentification</display-name>
    <web-resource-collection>
      <web-resource-name>Authentification</web-resource-name>
      <description/>
      <url-pattern>/AuthenticationServlet</url-pattern>
    </web-resource-collection>
    <web-resource-collection>
      <web-resource-name>Default Login page</web-resource-name>
      <description/>
      <url-pattern>/defaultLogin.jsp</url-pattern>
    </web-resource-collection>
    <web-resource-collection>
      <web-resource-name>Login page</web-resource-name>
      <description/>
      <url-pattern>/Login.jsp</url-pattern>
    </web-resource-collection>
    <user-data-constraint>
      <description>SSL</description>
      <transport-guarantee>NONE</transport-guarantee>
    </user-data-constraint>
  </security-constraint>

  <session-config>
    <cookie-config>
      <http-only>true</http-only>
      <secure>false</secure>
    </cookie-config>
    <tracking-mode>COOKIE</tracking-mode>
  </session-config>

  <resource-env-ref>
    <description>Content Repository</description>
    <resource-env-ref-name>jcr/repository</resource-env-ref-name>
    <resource-env-ref-type>javax.jcr.Repository</resource-env-ref-type>
    <lookup-name>java:/jca/jcr</lookup-name>
  </resource-env-ref>
</web-app>
